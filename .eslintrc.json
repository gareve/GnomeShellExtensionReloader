{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [  "eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "camelcase":"warn",
        "camelcase/ignoreDestructuring": "true"
    },
    "globals": {
        "imports": "readonly",
        "log": "readonly",
        "global": "readonly"
    }
}
